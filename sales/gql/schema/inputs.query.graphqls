input LoadInput {
  spCode: String!
  opDay: String!
  salesType: Int!
}

input FindInput {
  spCode: String!
  opDayStart: String!
  opDayEnd: String!
  salesTypeList: [Int!]!
  statusList: [Int!]!
}

type SalesQuery {
  load(input: LoadInput!): Inputs @goField(forceResolver: true)
  find(input: FindInput!): [Inputs!] @goField(forceResolver: true)
}
